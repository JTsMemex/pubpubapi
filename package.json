{
  "name": "pubpub-client",
  "version": "0.9.0",
  "description": "Unofficial PubPub API client for Node.js and the browser.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "postbuild": "pnpm attw --pack . && size-limit",
    "lint": "tsc",
    "ci": "rm -rf dist && npm run lint && pnpm run test && pnpm run build",
    "prepublish": "pnpm run ci",
    "release": "pnpm run ci && changeset publish",
    "prepare": "husky install",
    "size": "banal ./dist/index.js",
    "pretest": "pnpm run -C=core build-client-dev",
    "test": "firebase emulators:exec --only database --import ./core/.firebase/default-contents \"jest\""
  },
  "keywords": [
    "pubpub",
    "api",
    "sdk",
    "client",
    "publishing"
  ],
  "author": "Thomas F. K. Jorna",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.0",
    "@ts-rest/core": "^3.30.0",
    "@types/uuid": "^3.3.0",
    "crypto-js": "^4.1.1",
    "prosemirror-compress-pubpub": "0.0.3",
    "prosemirror-model": "^1.18.3",
    "prosemirror-state": "^1.4.2",
    "prosemirror-tables": "^1.3.4",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-view": "^1.29.2",
    "uuid": "^3.3.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.9.0",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/types": "^29.6.3",
    "@size-limit/esbuild": "^9.0.0",
    "@size-limit/esbuild-why": "^9.0.0",
    "@size-limit/file": "^9.0.0",
    "@swc/core": "^1.3.84",
    "@swc/jest": "^0.2.29",
    "@swc/types": "^0.1.4",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.15.3",
    "@types/react": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.20",
    "eslint": "^8.48.0",
    "firebase-tools": "^12.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "size-limit": "^9.0.0",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "80 kB",
      "import": "{ PubPub }",
      "gzip": true,
      "name": "gzip-ed"
    },
    {
      "path": "dist/index.js",
      "limit": "309 kB",
      "import": "{ PubPub }",
      "gzip": false,
      "name": "uncompressed"
    }
  ]
}
