{
  "name": "pubpub-client",
  "version": "0.8.2",
  "description": "Unofficial PubPub API client for Node.js and the browser.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "postbuild": "pnpm attw --pack .",
    "lint": "tsc",
    "ci": "rm -rf dist && pnpm run lint && pnpm run test && pnpm run build ",
    "prepublish": "npm run ci",
    "dev": "vitest",
    "release": "npm run ci && changeset publish",
    "prepare": "husky install",
    "size": "banal ./dist/index.js",
    "pretest": "pnpm run -C=core build-client-dev",
    "test": "firebase emulators:exec --only database --import ./core/.firebase/default-contents \"jest\""
  },
  "keywords": [
    "pubpub",
    "api",
    "sdk",
    "client",
    "publishing"
  ],
  "author": "Thomas F. K. Jorna",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.0",
    "@ts-rest/core": "^3.30.0",
    "@types/uuid": "^3.3.0",
    "crypto-js": "^4.1.1",
    "prosemirror-compress-pubpub": "0.0.3",
    "prosemirror-model": "^1.18.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-tables": "^1.3.2",
    "prosemirror-transform": "^1.2.5",
    "prosemirror-view": "^1.29.0",
    "uuid": "^3.3.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.9.0",
    "@babel/generator": "^7.22.15",
    "@babel/parser": "^7.22.16",
    "@babel/traverse": "^7.22.15",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/types": "^29.6.3",
    "@size-limit/esbuild": "^9.0.0",
    "@size-limit/esbuild-why": "^9.0.0",
    "@size-limit/file": "^9.0.0",
    "@swc/core": "^1.3.84",
    "@swc/jest": "^0.2.29",
    "@types/express": "4.17.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.15.3",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.20",
    "esbuild-analyzer": "^0.2.0",
    "esbuild-plugin-summary": "^0.0.2",
    "eslint": "^8.48.0",
    "express": "^4.17.3",
    "fast-folder-size": "^2.2.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "knex": "^2.5.1",
    "pako": "^2.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "readdirp": "^3.6.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "size-limit": "^9.0.0",
    "svelte": "^3.57.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "unplugin-swc": "^1.4.2",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.29.2"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "80 kB",
      "import": "{ PubPub }",
      "gzip": true,
      "name": "gzip-ed"
    },
    {
      "path": "dist/index.js",
      "limit": "309 kB",
      "import": "{ PubPub }",
      "gzip": false,
      "name": "uncompressed"
    }
  ]
}
